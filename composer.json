{
    "name": "stayallive/laravel-eloquent-uuid",
    "type": "library",
    "description": "Generate UUID for a Laravel Eloquent model attribute.",
    "keywords": [
        "laravel",
        "eloquent",
        "uuid"
    ],
    "homepage": "https://github.com/stayallive/laravel-eloquent-uuid",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Bouma",
            "email": "alex@bouma.me"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^9|^10|^11",
        "ramsey/uuid": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Stayallive\\Laravel\\Eloquent\\UUID\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9",
        "friendsofphp/php-cs-fixer": "^3.15"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --stop-on-failure",
        "test:ci": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix",
        "phpcs:ci": "vendor/bin/php-cs-fixer fix --dry-run --diff"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
